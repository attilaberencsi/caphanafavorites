{
  "name": "caphanafavorites",
  "version": "1.0.0",
  "description": "Favorites Application made usingwith CAP, HANA and Fiori",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2"
  },
  "scripts": {
    "start": "cds-serve",
    "build:hana": "cds build --for hana",
    "watch:hybrid" : "cds watch --profile hybrid",
    "deploy:hana:hybrid": "cds deploy --to hana --profile hybrid"
  },
  "private": true,
  "cds": {
    "requires": {
      "[hybrid]": {
        "db": "hana"
      }
    }
  }
}